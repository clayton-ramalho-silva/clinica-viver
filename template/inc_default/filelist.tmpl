<!--FILE_SETTINGS_START//-->
; this is formatted like WIN.INI
; please: do not use comments for value lines
icon_path 		= "img/icons/"
icon_name 		= "small_icon_{FILE_EXT}.gif"
file_size_round	= 1
file_size_space = " "
date_format		= "%m/%d/%y"
set_locale		= "de_DE@Euro"
<!--FILE_SETTINGS_END//-->
<div class="bloco-downloads baid{ID}">
    <section>
        [TITLE]<div class="subtitulo">[/TITLE]
                [TITLE]<h2>{TITLE}</h2>[/TITLE]
                [SUBTITLE]<h3>{SUBTITLE}</h3>[/SUBTITLE]
                [TITLE]</div>[/TITLE]
             [TITLE]<div class="divider"></div>[/TITLE]
  

        <ul class="downloads">
            <!--FILE_ENTRY_START//-->

            <li>
                <a href="{FILE_LINK}" title="Baixar Arquivo" target="_blank">
                    <i class="fas fa-download"></i>
                    [FILE_TITLE]{FILE_TITLE}[/FILE_TITLE][FILE_TITLE_ELSE]{FILE_NAME}[/FILE_TITLE_ELSE]
                </a>

            </li>

            <!--FILE_ENTRY_END//-->
        </ul>
    </section>
</div>


